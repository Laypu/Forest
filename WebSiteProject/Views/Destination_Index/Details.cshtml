    @{
        var DesModel = (IEnumerable<WebSiteProject.Models.F_Destination_Type>)ViewBag.F_Destination_Type;
        var BanTitle = DesModel.First().Destination_Type_Title1 + " " + DesModel.First().Destination_Type_Title2;
    }
    <div class="banner_title">
        <h2>@BanTitle</h2>
    </div>

</div>
<!--banner end-->
<!--page_info start-->
<div class="page_info">
    <div class="container">
        <div class="con_font">
            @DesModel.First().Destination_Type_Description;
        </div>
    </div>
</div>
<!--page_info end-->
<!--page_bookmark start-->
<div class="page_bookmark">
    <div class="container">

        <ul class="bookmark_item">
            <li><a href="javascript:void(0);" name="page_01" title="" class="active">Location</a></li>
            <li><a href="javascript:void(0);" name="page_02" title="">Featured points</a></li>
            <li><a href="javascript:void(0);" name="page_03" title="">Season</a></li>
            <li><a href="javascript:void(0);" name="page_04" title="">Charges</a></li>
        </ul>

        <div class="locaton">
            <ul>
                <li data-sr="enter bottom over 1.5s">
                    <div class="img"><img src="@Url.Content("~/img/page_icon_01.jpg")" alt="" /></div>
                    <div class="font">
                        <div class="title">Location</div>
                        <div class="con_font">@DesModel.First().Destination_Type_Location</div>
                    </div>
                </li>
                <li data-sr="enter bottom over 1.5s">
                    <div class="img"><img src="@Url.Content("~/img/page_icon_02.jpg")" alt="" /></div>
                    <div class="font">
                        <div class="title">Service hours</div>
                        <div class="con_font">@DesModel.First().Destination_Type_ServiceHours</div>
                    </div>
                </li>
                <li data-sr="enter bottom over 1.5s">
                    <div class="img"><img src="@Url.Content("~/img/page_icon_03.jpg")" alt="" /></div>
                    <div class="font">
                        <div class="title">Area</div>
                        <div class="con_font">@DesModel.First().Destination_Type_Area</div>
                    </div>
                </li>
                <li data-sr="enter bottom over 1.5s">
                    <div class="img"><img src="@Url.Content("~/img/page_icon_04.jpg")" alt="" /></div>
                    <div class="font">
                        <div class="title">Altitude</div>
                        <div class="con_font">@DesModel.First().Destination_Type_Altitude</div>
                    </div>
                </li>
            </ul>
        </div>

    </div>
</div>
<!--page_bookmark end-->
<!--page_content start-->
<div class="page_content">

    <!--page top start-->
    <div class="page_top">
        <div class="container">
            <div class="col-md-7 col-sm-6 col-xs-12 page_top_left">
