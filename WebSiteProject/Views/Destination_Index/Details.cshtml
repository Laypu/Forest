                        <div class="link"><a href="#" title="">#Walking & Hiking</a> &nbsp; <a href="#" title="">#Clamping</a> </div>
                    </div>
                </li>

            </ul>
        </div>
    </div>
    <!--article_list end-->
    <!--detail_map start-->
    <div class="detail_map">
        <iframe width="100%" height="400" frameborder="0" scrolling="no" marginheight="0" marginwidth="0" src="https://maps.google.com.tw/maps?f=q&hl=zh-TW&geocode=&q=@DesModel.First().Destination_Type_MapName&z=16&output=embed&t="></iframe>
    </div>
    <!--detail_map end-->
</div>
<!--page_content end-->
<!--breadcrumbs start-->
<div class="breadcrumbs">
    <div class="container">
        <div class="title">You are here</div>
        <div class="font"><a href="index.html" title="Home">Home</a> / <a href="destinations.html" title="Destinations">Destinations</a> / Hehuanshan Forest Recreation Area</div>
