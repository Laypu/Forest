@model AuthoritySearchModel
<script src="@Url.Content("~/Scripts/datatable.js")"></script>
<script src="@Url.Content("~/Scripts/custom.js")"></script>
<script src="@Url.Content("~/Scripts/bootbox.min.js")"></script>
<!--page bar start-->
<div class="page-bar all_width">
    <ul class="page-breadcrumb">
        <li>
            <span href="#">Home</span>
            <i class="fa fa-circle"></i>
            <span href="#">系統管理</span>
            <i class="fa fa-circle"></i>
            <span href="#">流量分析 </span>
        </li>
    </ul>
</div>
<!--page bar end-->
<div class="member_title">欄位名稱設定</div>
<div class="portlet light bordered">
    <!--set item start-->
    <div class="table-toolbar">
        <div class="col-md-6 col-sm-12 col-xs-12">
            <div class="display_inline">
                @if (ViewBag.isClick == "Y")
                {
                    <button type="button" class="btn blue" id="btn_tositeflow">流量分析頁面<i class="fa fa-wrench" style="margin-left:2px"></i></button>
                }
                else
                {
                    <p class="btn_03">流量分析頁面</p>
                }
                <button type="button" class="btn blue" id="btn_siteflow">設定<i class="fa fa-wrench" style="margin-left:2px"></i></button>
            </div>
        </div>

    </div>

</div>

<!--message end-->
@section scripts {
    <script>

        $(document).ready(function () {
            $("#btn_tositeflow").click(function () {
                window.open(
                    '@ViewBag.SiteFlowURL',
                    '_blank'
                );
            })
            $("#btn_siteflow").click(function () {
                 CreatePost('@Url.Action("SiteFlowEdit")', null);
              })
        });

    </script>
}
